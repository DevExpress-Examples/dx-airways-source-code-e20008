                                                <uc1:PersonName ID="PersonName1" runat="server" />
                                            </LoggedInTemplate>
                                        </asp:LoginView>
                                        &nbsp;&nbsp;&nbsp;
                                        <dxe:ASPxHyperLink ID="lnkSiteMap" runat="server" Text="" 
                                            ImageUrl="Images/SiteMap.gif" ImageWidth="15px">
                                        </dxe:ASPxHyperLink>
                                    </td>
                                </tr>
                                <tr>
                                    <td align="right">
                                        <asp:LoginStatus ID="secAuthLink" runat="server" LoginText="Sign In" 
                                            LogoutText="Sign Out " />
