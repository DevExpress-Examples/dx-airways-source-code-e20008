                                        <dxe:ASPxHyperLink ID="lnkHomeLogo" runat="server" Text="" 
                                            ImageUrl="Images/DXairways-SharpWings-Duotone-version2.png" 
                                            ImageWidth="425px">
                                        </dxe:ASPxHyperLink>
                                    </td>
                                    <td align="right" valign="top">
                                        <asp:LoginView ID="secAuthView" runat="server">
